{
    "name": "jasufo/systemd-logger",
    "description": "Log your php app to systemd journal, without changing your app",
    "type": "library",
    "license": "Jasufo",
    "require": {
        "psr/log": "^1.1",
        "php": ">=7.0",
        "symfony/symfony": "3.4.*",
        "symfony/framework-bundle": "^3.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "SystemdLoggerBundle\\": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
