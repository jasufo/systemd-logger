{
    "name": "jasufo/systemd-logger",
    "description": "Log your php app to systemd journal, without changing your app",
    "type": "library",
    "license": "Jasufo",
    "require": {
        "psr/log": "^1.1"
    },
    "autoload": {
        "exclude-from-classmap": ["/Tests/"],
        "psr-4": {
            "Jasufo\\SystemdLoggerBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasufo\\SystemdLoggerBundle\\": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
